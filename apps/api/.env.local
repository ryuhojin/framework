# API 로컬 환경 예시
APP_ENV=local
NODE_ENV=development
API_HOST=0.0.0.0
API_PORT=3000
APP_BASE_URL=http://localhost:3000

# 데이터베이스 설정
DB_VENDOR=postgres
DB_HOST=127.0.0.1
DB_PORT=5432
DB_USER=app_user
DB_PASSWORD=app_password
DB_NAME=app_db
DB_SCHEMA=public
DB_DISABLE=false

# 보안/로그
CORS_ALLOWED_ORIGINS=http://localhost:3000,http://localhost:3001
ENABLE_HELMET=true
CSP_ENABLED=false
LOG_LEVEL=debug
